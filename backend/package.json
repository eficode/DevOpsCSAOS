{
  "name": "survey-backend",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "start-robot": "cross-env NODE_ENV=endtoend npx sequelize-cli db:migrate && cross-env NODE_ENV=endtoend node server.js",
    "dev": "nodemon server.js",
    "test": "cross-env NODE_ENV=test SECRET_FOR_TOKEN=abcdefg jest --verbose --runInBand --detectOpenHandles --forceExit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "reset-test-db": "export NODE_ENV=test && dropdb test_db; createdb test_db && npx sequelize-cli db:migrate",
    "create-dev-db": "export NODE_ENV=development && createdb dev_db && npx sequelize-cli db:migrate"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@hubspot/api-client": "^3.4.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.4.0",
    "uuid-validate": "0.0.3"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.1"
  }
}
